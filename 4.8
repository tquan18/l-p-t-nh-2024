input_string = input("Nhập một dãy các từ (cách nhau bởi dấu trống hoặc tab): ")

danh_sach_tu = input_string.split()

do_dai_max = max(len(tu) for tu in danh_sach_tu)

cac_tu_dai_nhat = [tu for tu in danh_sach_tu if len(tu) == do_dai_max]

print("Các từ dài nhất trong dãy vừa nhập:")
for tu in cac_tu_dai_nhat:
    print(tu)
