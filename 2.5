# Nhập một số tự nhiên lớn hơn 0 từ bàn phím
try:
    n = int(input("Nhập một số tự nhiên (n > 0): "))
    
    if n <= 0:
        print("Giá trị nhập vào phải là một số tự nhiên lớn hơn 0.")
    else:
        # In các số tự nhiên giảm dần từ n đến 0
        for i in range(n, -1, -1):
            print(i)
except ValueError:
    print("Giá trị nhập vào không hợp lệ. Vui lòng nhập một số tự nhiên.")
