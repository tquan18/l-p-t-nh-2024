def Sequential_Search(dlist, item):
    """
    Hàm tìm kiếm tuyến tính trong danh sách dlist.
    Trả về (True, vị trí) nếu tìm thấy, (False, -1) nếu không tìm thấy.
    """
    for index, value in enumerate(dlist):
        if value == item:
            return (True, index)
    return (False, -1)
