def cong(a, b):
    """Hàm tính tổng của hai số."""
    return a + b

def tru(a, b):
    """Hàm tính hiệu của hai số."""
    return a - b

def nhan(a, b):
    """Hàm tính tích của hai số."""
    return a * b

def chia(a, b):
    """Hàm tính thương của hai số."""
    if b == 0:
        raise ValueError("Không thể chia cho số 0")
    return a / b
