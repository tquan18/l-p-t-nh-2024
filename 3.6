def thay_doi_gia_tri(x):
    """
    Hàm này thay đổi giá trị của tham số x.
    """
    print(f"Giá trị của x trước khi thay đổi: {x}")
    x = x + 10
    print(f"Giá trị của x sau khi thay đổi: {x}")

y = 5
print(f"Giá trị của y trước khi gọi hàm: {y}")

thay_doi_gia_tri(y)

print(f"Giá trị của y sau khi gọi hàm: {y}")
