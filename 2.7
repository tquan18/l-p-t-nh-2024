def tao_dictionary(n):
    """
    Hàm tạo ra một dictionary chứa các cặp giá trị (i, i*i) từ 1 đến n.
    :param n: Số nguyên.
    :return: Dictionary chứa các cặp giá trị (i, i*i).
    """
    dictionary = {}
    for i in range(1, n + 1):
        dictionary[i] = i * i
    return dictionary

# Nhập số nguyên từ bàn phím
try:
    n = int(input("Nhập một số nguyên: "))
    ket_qua = tao_dictionary(n)
    print(ket_qua)
except ValueError:
    print("Giá trị nhập vào không hợp lệ. Vui lòng nhập một số nguyên.")
