def dem_ky_tu(s):
    dictionary = {}
    for char in s:
        if char in dictionary:
            dictionary[char] += 1
        else:
            dictionary[char] = 1
    return dictionary
input_string = input("Nhập một chuỗi ký tự: ")
result = dem_ky_tu(input_string)
print("Số ký tự trong chuỗi:", result)
